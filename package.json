{
  "name": "langchain-agents-ts",
  "version": "0.1.0",
  "description": "TypeScript version of LangChain agents project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "pkill -f 'node.*2024' 2>/dev/null || true && sleep 1 && npx @langchain/langgraph-cli up",
    "dev": "pkill -f 'node.*2024' 2>/dev/null || true && sleep 1 && npx @langchain/langgraph-cli dev",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "install:deps": "pnpm install",
    "add:dep": "pnpm add",
    "add:dev": "pnpm add -D",
    "langgraph:dev": "pkill -f 'node.*2024' 2>/dev/null || true && sleep 1 && npx @langchain/langgraph-cli dev",
    "langgraph:build": "langgraph build"
  },
  "keywords": [
    "langchain",
    "agents",
    "typescript",
    "openai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "*",
    "@langchain/langgraph": "*",
    "@langchain/openai": "*",
    "dotenv": "*",
    "langchain": "*",
    "zod": "*"
  },
  "devDependencies": {
    "@langchain/langgraph-cli": "^1.1.2",
    "@types/jest": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "jest": "*",
    "nodemon": "*",
    "prettier": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*"
  }
}